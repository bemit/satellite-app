{
    "name": "orbiter/satellite-app",
    "license": "MIT",
    "description": "Easy to use Event & Middleware Mini-Framework, powered by popular projects and PSR's",
    "type": "project",
    "require": {
        "php": ">=7.4.1||>=8",
        "ext-json": "*",
        "psr/event-dispatcher": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-message": "^1.0",
        "orbiter/satellite": "~0.2.0",
        "orbiter/satellite-route": "~0.2.1",
        "orbiter/satellite-console": "~0.3.0",
        "orbiter/satellite-response": "~0.1.3",
        "orbiter/annotations-util": "~0.5.0",
        "middlewares/request-handler": "^2.0.0",
        "middlewares/payload": "^3.0.0",
        "php-di/php-di": "6.3.3",
        "vlucas/phpdotenv": "^5.1.0",
        "monolog/monolog": "^2.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "orbiter/satellite-whoops": "~0.1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
