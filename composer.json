{
    "name": "orbiter/satellite-app",
    "license": "MIT",
    "description": "Easy to use Event & Middleware Mini-Framework, powered by popular projects and PSR's",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "psr/event-dispatcher": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-message": "^1.0",
        "orbiter/satellite": "0.1.x",
        "orbiter/satellite-route": "0.1.x",
        "orbiter/satellite-console": "0.2.x",
        "orbiter/satellite-response": "0.1.x",
        "orbiter/annotations-util": "0.4.x",
        "orbiter/middleware-twig": "dev-master",
        "middlewares/request-handler": "^1.4",
        "middlewares/payload": "^2.1",
        "php-di/php-di": "6.3.0",
        "vlucas/phpdotenv": "^5.1.0",
        "monolog/monolog": "^2.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "orbiter/satellite-whoops": "0.1.x"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
